# SELECT ?thing (COUNT(?s) as ?count) WHERE {
# 	GRAPH <http://mu.semte.ch/graphs/public> {
# 		?s a ?thing.
# 		?s ?p ?o.
# 	}
# } GROUP BY ?thing ORDER BY DESC(?count)

DELETE {
  GRAPH <http://mu.semte.ch/graphs/public> {
    ?s ?p ?o.
    ?oo ?pp ?s.
