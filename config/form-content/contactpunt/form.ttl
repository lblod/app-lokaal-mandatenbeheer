@prefix form: <http://lblod.data.gift/vocabularies/forms/>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix mu: <http://mu.semte.ch/vocabularies/core/>.
@prefix displayTypes: <http://lblod.data.gift/display-types/>.
@prefix schema: <http://schema.org/>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix nie: <http://www.semanticdesktop.org/ontologies/2007/01/19/nie#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix ext: <http://mu.semte.ch/vocabularies/ext/>.
@prefix vcard: <http://www.w3.org/2006/vcard/ns#>.

ext:faxF
    a form:Field;
    form:displayType displayTypes:defaultInput;
    sh:group ext:contactpuntPG;
    sh:name "Fax";
    sh:order 4;
    sh:path schema:faxNumber.
ext:aanschrijfprefixF
    a form:Field;
    form:displayType displayTypes:defaultInput;
    sh:group ext:contactpuntPG;
    sh:name "Aanschrijfprefix";
    sh:order 2;
    sh:path vcard:honorific-prefix .
ext:websiteF
    a form:Field;
    form:displayType displayTypes:remoteUrls;
    sh:group ext:contactpuntPG;
    sh:name "Website";
    sh:order 8;
    sh:path foaf:page.
ext:voornaamF
    a form:Field;
    form:displayType displayTypes:defaultInput;
    sh:group ext:contactpuntPG;
    sh:name "Voornaam";
    sh:order 6;
    sh:path foaf:firstName .
ext:emailF
    a form:Field;
    form:displayType displayTypes:defaultInput;
    sh:group ext:contactpuntPG;
    sh:name "Email";
    sh:order 3;
    sh:path schema:email.
ext:typeF
    a form:Field;
    form:displayType displayTypes:defaultInput;
    sh:group ext:contactpuntPG;
    sh:name "Type";
    sh:order 10;
    sh:path schema:contactType .
ext:achternaamF
    a form:Field;
    form:displayType displayTypes:defaultInput;
    sh:group ext:contactpuntPG;
    sh:name "Achternaam";
    sh:order 7;
    sh:path foaf:familyName.
ext:telefoonF
    a form:Field;
    form:displayType displayTypes:defaultInput;
    sh:group ext:contactpuntPG;
    sh:name "Telefoon";
    sh:order 9;
    sh:path schema:telephone.
ext:contactpuntPG
    a form:PropertyGroup; sh:name "Contactpunt"; sh:order 1 .
ext:naamF
    a form:Field;
    form:displayType displayTypes:defaultInput;
    sh:group ext:contactpuntPG;
    sh:name "Roepnaam";
    sh:order 5;
    sh:path foaf:name .

ext:form
    a form:Form, form:TopLevelForm;
    form:includes
        ext:faxF,
        ext:aanschrijfprefixF,
        ext:websiteF,
        ext:voornaamF,
        ext:emailF,
        ext:typeF,
        ext:achternaamF,
        ext:telefoonF,
        ext:naamF;
    sh:group ext:contactpuntPG ;
    form:initGenerator ext:contactpuntG ;
    ext:label "contactpunt" .

ext:contactpuntG a form:Generator;
  form:prototype [
    form:shape [
      a schema:ContactPoint
    ]
  ];
  form:dataGenerator form:addMuUuid.